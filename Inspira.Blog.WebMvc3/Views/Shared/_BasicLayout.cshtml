        </div>
		<!-- /footer - copyright info -->
    </div>

	@* yui *@
	@* <script type="text/javascript" src="http://yui.yahooapis.com/3.0.0pr2/build/yui/yui-min.js" type="text/javascript"></script> *@

	@* yui jquery / plugins *@
	@*
	<script type="text/javascript" src="@Url.Content("~/assets/js/mvc/jquery-1.4.1-vsdoc.js")"></script>
	*@

